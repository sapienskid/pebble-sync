{
  "name": "pebble-sync",
  "version": "1.0.0",
  "description": "Import Pebble notes into atomic notes and link them back to your daily notes.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "plugin",
    "pebble",
    "sync",
    "notes"
  ],
  "author": "sapienskid",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.19.12",
    "obsidian": "latest",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  }
}
